{
  "name": "landr",
  "version": "0.0.0",
  "description": "Source code = website",
  "homepage": "https://github.com/resin-io/landr",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io/landr.git"
  },
  "scripts": {
    "start": "yarn run watch",
    "watch": "lerna run --parallel watch",
    "build": "lerna run --parallel build",
    "publish": "lerna publish",
    "deploy": "lerna run deploy --scope landr",
    "postinstall": "lerna bootstrap --npm-client=yarn",
    "prettier": "prettier --single-quote --write \"packages/*/{index.js,+(lib|bin|www)/**/*.js}\"",
    "validate-schema": "ajv -s docs/schema/index.json -d 'docs/schema/examples/*.json'"
  },
  "devDependencies": {
    "ajv-cli": "2.1.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-runtime": "^6.23.0",
    "lerna": "^2.0.0-rc.5",
    "prettier": "1.5.3"
  },
  "dependencies": {
    "regenerator-runtime": "0.11.0"
  }
}
