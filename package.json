{
  "name": "landr",
  "version": "6.23.0",
  "description": "Generate great websites for your projects with zero configuration",
  "main": "src/cli/index.js",
  "bin": {
    "landr": "./bin/run"
  },
  "scripts": {
    "bot:dev": "nodemon",
    "bot:start": "probot run ./bot/index.js",
    "cli:docs": "oclif-dev readme",
    "lint": "eslint --ext .js,.jsx lib bot test scripts && csslint lib/**/*.css",
    "unit": "nyc --exclude scripts --exclude test --reporter=html --extension .js --extension .jsx --compact=false ava",
    "build": "./bin/run build",
    "test-mixpanel": "LANDR_MIXPANEL_TOKEN=04ad3098829fcd7c5700d75cea220530 ./bin/run build",
    "test-npm": "rm -f land-*.tgz && npm pack && npm install --global ./landr-$npm_package_version.tgz && landr build && rm landr-*.tgz",
    "test": "npm run lint && npm run unit && npm run build && npm run test-npm && npm run test-mixpanel",
    "start": "npm run build",
    "start:dev": "./bin/run build --meta=./meta/sound.meta.json --verbose && http-server ./landr-dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/balena-io/landr.git"
  },
  "keywords": [
    "website",
    "generation",
    "docs",
    "project",
    "ssg"
  ],
  "author": "Balena.io <hello@balena.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/balena-io/landr/issues"
  },
  "files": [
    "/bin",
    "/oclif.manifest.json",
    "/lib",
    "/bot",
    "/default-theme.json",
    "/skeleton"
  ],
  "homepage": "https://github.com/balena-io/landr",
  "ava": {
    "verbose": true,
    "failWithoutAssertions": true,
    "files": [
      "test/**/*.spec.js",
      "test/**/*.spec.jsx"
    ],
    "require": [
      "./scripts/babel-test-register.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@oclif/dev-cli": "^1.22.2",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "ava": "^2.1.0",
    "babel-loader": "^8.0.6",
    "csslint": "^1.0.5",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^8.3.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "js-yaml": "^3.13.1",
    "jsdom": "^15.1.1",
    "nock": "^12.0.0",
    "nodemon": "^2.0.0",
    "nyc": "^14.1.1",
    "puppeteer": "^1.20.0"
  },
  "dependencies": {
    "@babel/register": "^7.4.0",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@oclif/command": "^1.5.19",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-help": "^2.2.3",
    "@reach/router": "^1.2.1",
    "@react-google-maps/api": "^1.13.0",
    "bluebird": "^3.7.2",
    "chalk": "^2.4.2",
    "date-fns": "^2.0.1",
    "file-type": "^12.0.1",
    "generatorics": "^1.1.0",
    "git-branch": "^2.0.1",
    "gitinfo": "2.2.0",
    "handlebars": "^4.1.2",
    "handlebars-helpers": "^0.10.0",
    "hex-to-rgba": "^2.0.1",
    "lodash": "^4.17.15",
    "markdown": "^0.5.0",
    "mixpanel-browser": "^2.29.0",
    "mkdirp": "^0.5.1",
    "netlify": "^6.1.18",
    "node-fetch": "^2.6.0",
    "node-vibrant": "^3.1.3",
    "null-loader": "^4.0.1",
    "probot": "^9.11.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-hash-link": "^1.2.2",
    "react-static": "^7.4.2",
    "react-static-plugin-reach-router": "^7.0.10",
    "react-static-plugin-react-router": "^7.2.3",
    "react-static-plugin-sitemap": "^7.0.10",
    "react-static-plugin-styled-components": "^7.5.0",
    "react-typist": "^2.0.5",
    "recursive-copy": "^2.0.10",
    "rendition": "^21.7.1",
    "replace-string": "^3.0.0",
    "rfdc": "^1.1.4",
    "rimraf": "^2.6.3",
    "scrutinizer": "^3.4.0",
    "serialize-error": "^6.0.0",
    "shelljs": "^0.8.3",
    "smee-client": "^1.1.0",
    "styled-components": "^5.2.1",
    "tmp": "^0.1.0",
    "typed-errors": "^1.1.0",
    "uuid": "^7.0.3"
  },
  "nodemonConfig": {
    "verbose": true,
    "exec": "npm run bot:start",
    "watch": [
      ".env",
      "lib",
      "bin",
      "bot"
    ],
    "ignore": [
      ".tmp/",
      ".landr-store/"
    ]
  },
  "oclif": {
    "commands": "./lib/cli/commands",
    "bin": "landr",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/cli/hooks/init.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "versionist": {
    "publishedAt": "2021-10-07T09:43:48.856Z"
  }
}
